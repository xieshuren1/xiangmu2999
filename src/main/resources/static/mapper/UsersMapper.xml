  	<if test="u.jid!=null and u.jid!=0 ">
  	and u.jid=#{u.jid}
  	</if>
  	<if test="e.phone!=null and e.phone!='' ">
  	and e.phone like CONCAT('%',#{e.phone},'%')
  	</if>
  </where>LIMIT #{pageNum},#{maxPage}
  </select>
