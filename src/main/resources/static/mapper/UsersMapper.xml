  <select id="findUsers" parameterType="com.guigu.yaopin.dyj.pojo.UsersQuervo" resultMap="BaseResultMap">
  SELECT * from users u,emp e,juese j 
  <where>
  	and  e.uid=u.userid  and u.jid=j.jid
  	<if test="e.empname!=null and e.empname!='' ">
